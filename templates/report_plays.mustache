<h5>
    {{#str}} report:plays:header, mod_videolesson{{/str}} ({{{total}}}) {{#user}} - {{user.fullname}} {{/user}}
</h5>

<table class="table">
  <thead>
    <tr>
      <th scope="col">{{#str}} report:plays:col:user, mod_videolesson{{/str}}</th>
      <th scope="col">{{#str}} report:plays:col:time, mod_videolesson{{/str}}</th>
      <th scope="col">{{#str}} report:plays:col:duration, mod_videolesson{{/str}}</th>
      <th scope="col">{{#str}} report:plays:col:data, mod_videolesson{{/str}}</th>
    </tr>
  </thead>
  <tbody>
    {{#data}}
      <tr>
        <td>{{{userid}}}</td>
        <td><a href="{{{url}}}">{{{userdate}}}</a></td>
        <td>{{{watchduration}}}  ({{#str}} report:plays:progress, mod_videolesson, {{{progress}}}{{/str}})</td>
        <td>
            <div id="mod-videolesson-chart-container">
                <div data-ranges="{{{ranges}}}" data-vduration="{{{duration}}}" class="mod-videolesson-chart m-auto" style="max-width: 900px;"></div>
            </div>
        </td>
      </tr>
    {{/data}}
  </tbody>
</table>
