      <th scope="col">{{#str}}report:play:visibility:col:active, mod_videolesson{{/str}}</th>
    </tr>
  </thead>
  <tbody>
    {{#sessiondata.visibility}}
      <tr>
        <td>{{{timestamp}}}</td>
        <td>{{{position}}}</td>
        <td>{{#active}}{{#str}}report:play:visibility:yes, mod_videolesson{{/str}}{{/active}}{{^active}}{{#str}}report:play:visibility:no, mod_videolesson{{/str}}{{/active}}</td>
      </tr>
    {{/sessiondata.visibility}}
    {{^sessiondata.visibility}}
    <tr>
        <td colspan="3">{{#str}}report:nodata, mod_videolesson{{/str}}</td>
