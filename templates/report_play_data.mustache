<p>
  {{#str}}report:play:user, mod_videolesson{{/str}} {{{user.fullname}}} <br>
  {{#str}}report:play:time, mod_videolesson{{/str}} {{{sessiondata.timestamp}}} <br>
  {{#str}}report:play:city, mod_videolesson{{/str}} {{{sessiondata.city}}} <br>
  {{#str}}report:play:country, mod_videolesson{{/str}} {{{sessiondata.country}}} <br>
  {{#str}}report:play:ip, mod_videolesson{{/str}} {{{sessiondata.ip}}} <br>
  {{#str}}report:play:platform, mod_videolesson{{/str}} {{{sessiondata.platform}}} <br>
  {{#str}}report:play:browser, mod_videolesson{{/str}} {{{sessiondata.browser}}} <br>
  {{#str}}report:play:os, mod_videolesson{{/str}} {{{sessiondata.os}}} <br>
</p>

<hr>

<h5>
  {{#str}}report:play:playback:header, mod_videolesson{{/str}}
</h5>

<table class="table">
  <thead>
    <tr>
      <th scope="col">{{#str}}report:play:playback:col:type, mod_videolesson{{/str}}</th>
      <th scope="col">{{#str}}report:play:playback:col:time, mod_videolesson{{/str}}</th>
      <th scope="col">{{#str}}report:play:playback:col:position, mod_videolesson{{/str}}</th>
    </tr>
  </thead>
  <tbody>
    {{#sessiondata.playbackEvents}}
      <tr>
        <td>{{{event_type}}}</td>
        <td>{{{timestamp}}}</td>
        <td>{{{position}}}</td>
      </tr>
    {{/sessiondata.playbackEvents}}
    {{^sessiondata.playbackEvents}}
    <tr>
        <td colspan="3">{{#str}}report:nodata, mod_videolesson{{/str}}</td>
    </tr>
    {{/sessiondata.playbackEvents}}
  </tbody>
</table>

<hr>

<h5>
  {{#str}}report:play:seek:header, mod_videolesson{{/str}}
</h5>

<table class="table">
  <thead>
    <tr>
      <th scope="col">{{#str}}report:play:seek:col:time, mod_videolesson{{/str}}</th>
      <th scope="col">{{#str}}report:play:seek:col:startposition, mod_videolesson{{/str}}</th>
      <th scope="col">{{#str}}report:play:seek:col:newposition, mod_videolesson{{/str}}</th>
    </tr>
  </thead>
  <tbody>
    {{#sessiondata.seekEvents}}
      <tr>
        <td>{{{timestamp}}}</td>
        <td>{{{start}}}</td>
        <td>{{{position}}}</td>
      </tr>
    {{/sessiondata.seekEvents}}
    {{^sessiondata.seekEvents}}
    <tr>
        <td colspan="3">{{#str}}report:nodata, mod_videolesson{{/str}}</td>
    </tr>
    {{/sessiondata.seekEvents}}
  </tbody>
</table>
<hr>

