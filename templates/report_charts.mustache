<ul class="nav nav-tabs" id="charttab" role="tablist">
    {{#tablabel}}
    <li class="nav-item" role="presentation">
        <a class="nav-link {{#active}}active{{/active}}" id="{{id}}-tab" data-toggle="tab" data-target="#{{id}}" role="tab" aria-controls="{{id}}" aria-selected="true">{{label}}</a>
    </li>
    {{/tablabel}}
</ul>
<div class="tab-content" id="charttabs">
    {{#tabcontent}}
    <div class="tab-pane fade {{#active}}show active{{/active}} py-4 px-2" id="{{id}}" role="tabpanel" aria-labelledby="{{id}}-tab">{{{content}}}</div>
    {{/tabcontent}}
</div>
