<h5>
  {{#str}}report:impressions:header, mod_videolesson{{/str}} ({{{total}}})
</h5>

<table class="table">
  <thead>
    <tr>
      <th scope="col">{{#str}}report:impressions:col:user, mod_videolesson{{/str}}</th>
      <th scope="col">{{#str}}report:impressions:col:time, mod_videolesson{{/str}}</th>
      <th scope="col">{{#str}}report:impressions:col:others, mod_videolesson{{/str}}</th>
    </tr>
  </thead>
  <tbody>
    {{#data}}
      <tr>
        <td>{{{userid}}}</td>
        <td>{{{timestamp}}}</td>
        <td>{{{combined}}}</td>
      </tr>
    {{/data}}
  </tbody>
</table>
